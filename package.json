{
  "name": "expo-reanimated-av-player",
  "version": "0.1.0",
  "description": "▶️ A high-performance, interactive and customizable video player control, built upon Reanimated v2 & GestureHandler v2.",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "expo-av-player.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/alantoa/expo-reanimated-av-player",
  "author": "alantoa <toacncom@gmail.com> (https://github.com/alantoa/expo-reanimated-av-player)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alantoa/expo-reanimated-av-player/issues"
  },
  "homepage": "https://github.com/alantoa/expo-reanimated-av-player#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.67.3",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "expo": "^44.0.6",
    "expo-av": "^10.2.1",
    "husky": "^6.0.0",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "16.13.1",
    "react-native": "^0.67.4",
    "react-native-builder-bob": "^0.18.0",
    "release-it": "^14.2.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@react-navigation/core": "^6.2.1",
    "expo-screen-orientation": "~4.1.1",
    "lottie-react-native": "^5.0.1",
    "react-native-awesome-slider": "^2.0.4",
    "react-native-gesture-handler": "~2.3.2",
    "react-native-reanimated": "~2.6.0",
    "react-native-safe-area-context": "^4.2.4"
  },
  "peerDependencies": {
    "@react-navigation/core": "*",
    "expo": ">=44.0.0",
    "expo-av": ">=11.0.0",
    "expo-screen-orientation": ">=4.0.0",
    "lottie-react-native": ">=4.0.0",
    "react": "*",
    "react-native": "*",
    "react-native-awesome-slider": ">=2.0.0",
    "react-native-gesture-handler": ">=2.0.0",
    "react-native-reanimated": ">=2.0.0",
    "react-native-safe-area-context": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
